client
verb 3
dev tun
persist-tun
persist-key
proto udp4
keepalive 10 60
inactive 3600
ping-timer-rem
sndbuf 524288
rcvbuf 524288
remote {{ vpn_remote_ip }}
resolv-retry infinite
rport {{ vpn_port }}
topology subnet
nobind
cipher AES-256-GCM
ncp-ciphers AES-256-GCM
auth SHA512
key-direction 1
remote-cert-eku 1.3.6.1.5.5.7.3.1
auth-user-pass
{% raw %}
<ca>
{{.CA}}
</ca>
<cert>
{{.Cert}}
</cert>
<key>
{{.Key}}
</key>
<tls-auth>
{{.TLSAuth}}
</tls-auth>
{% endraw %}
