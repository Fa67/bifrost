  "APIClient": {
    "ClientCertFile": "./client.crt",
    "ClientKeyFile": "./client.key",
    "SelfSignedServerCertFile": "./server.crt",
    "APISecretHeader": "X-Playground-Api-Secret",
    "APISecretValue": ""
