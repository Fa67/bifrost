{
  "Debug": true,
  "Port": 9000,
  "BindAddress": "",
  "LogFile": "",
  "APIServerURL": "https://localhost:9090",
  "StaticContent": "./static",
  "AdminUsers": [ "morrildl@playground.global" ],
  "Session": {
    "SessionCookieID": "PlaygroundSessionID",
    "OAuth": {
      "Issuer": "accounts.google.com",
